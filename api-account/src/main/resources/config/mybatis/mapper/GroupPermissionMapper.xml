<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.nnfs.api.account.mapper.GroupPermissionMapper">
	<insert id="create" parameterType="GroupPermission">
		insert into t_group_permission (group_id,permission_id) values (#{groupId},#{permissionId})
	</insert>

	<update id="update" parameterType="Group">
		update t_group_permission set permission_id =#{permissionId}
		where group_id = #{groupId}
	</update>

	<delete id="delete" parameterType="String">
		delete from t_group_permission where group_id = #{groupId}
	</delete>
</mapper>